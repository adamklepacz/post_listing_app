{
  "homepage": "https://github.com/adamklepacz/post_listing_app_demo",
  "name": "posts_listing_app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.7.0",
    "classnames": "2.2.6",
    "history": "4.10.1",
    "node-sass": "7.0.0",
    "prop-types": "15.7.2",
    "react-dom": "16.12.0",
    "react-image": "2.2.1",
    "react-paginate": "6.3.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "react": "16.12.0",
    "typeface-roboto": "0.0.75"
  },
  "scripts": {
    "build": "react-scripts build",
    "check": "npm-run-all lint format",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "format:fix": "prettier --write \"./src/**/*.{js,jsx}\"",
    "format": "prettier --check \"./src/**/*.{js,jsx}\"",
    "lint:fix": "eslint ./ --ext \".js,.jsx\" --max-warnings=0 --fix",
    "lint": "eslint ./ --ext \".js,.jsx\" --max-warnings=0",
    "predeploy": "npm run build",
    "start": "react-scripts start",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run check"
    }
  },
  "devDependencies": {
    "eslint": "6.6.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "gh-pages": "2.1.1",
    "husky": "3.1.0",
    "lint-staged": "9.4.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1"
  },
  "lint-staged": {
    "*.{js,jsx,scss,css,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  }
}
